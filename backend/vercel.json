{
  "version": 2,
  "builds": [
    {
      "src": "api/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/server.js"
    }
  ],
  "build": {
    "env": {
      "NODE_ENV": "production"
    },
    "npm": {
      "dev": "false"
    }
  }
}
